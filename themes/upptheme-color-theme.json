	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Preprocessor Comments",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation.definition.typeparameters",
				"storage.type.function.arrow",
			],
			"settings": {
				"foreground": "#00a0a0"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Object properties",
			"scope": [
				"variable.object.property",
				"variable.other.object.property",
				"variable.other.property",
				"support.variable.property",
				"meta.object-literal.key",
				"constant.other.symbol.hashkey"
			],
			"settings": {
				"foreground": "#8000ff"
			}
		},
		{
			"name": "Meta member",
			"scope": [
				"meta.object.member",
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Variable use",
			"scope": [
				"variable.other.object",
				"variable.other.readwrite",
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Instance variables",
			"scope": [
				"variable.other.object.instance",
				"variable.other.readwrite.instance punctuation.definition.variable",
				"variable.other.readwrite.instance",
			],
			"settings": {
				"foreground": "#a8a800"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"variable.other.constant",
			],
			"settings": {
				"foreground": "#48a0ff"
			}
		},
		{
			"name": "Constant definition",
			"scope": [
				"meta.definition.variable variable.other.constant",
			],
			"settings": {
				"foreground": "#b30000"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ff9122"
			}
		},
		{
			"name": "Function calls",
			"scope": [
				"meta.function-call entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#80b000"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#0080c0"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter",
				"variable.other.block"
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Braces",
			"scope": [
				"meta.brace.round"
			],
			"settings": {
				"foreground": "#ff80ff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#b30000"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#0080c0"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Punctuation template",
			"scope": "punctuation.definition.template-expression",
			"settings": {
				"foreground": "#bb77ff"
			}
		},
		{
			"name": "Braces {}",
			"scope": [
				"punctuation.definition.block",
				"punctuation.section.scope",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#bb77ff"
			}
		},
		{
			"name": "Brackets []",
			"scope": [
				"meta.brace.square",
				"punctuation.section.array",
			],
			"settings": {
				"foreground": "#db6262"
			}
		},
		{
			"name": "Import Components",
			"scope": "meta.import variable.other.readwrite.alias",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Import Function Components",
			"scope": "meta.import meta.block variable.other.readwrite.alias",
			"settings": {
				"foreground": "#ff9122"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"punctuation.definition.tag",
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": [
				"entity.name.tag",
				"support.class.component"
			],
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#8000ff"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
