{
	"name": "upptheme",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorLineNumber.foreground": "#aca899",
        "editor.selectionBackground": "#e4e4ff",
        "editor.selectionHighlightBackground": "#ffe4ffcc",
        "editor.wordHighlightBackground": "#ffe4ffcc",
        "editor.wordHighlightStrongBackground": "#ffe4ffcc",
        "list.activeSelectionIconForeground": "#fff"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Preprocessor Comments",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation.definition.typeparameters",
				"storage.type.function.arrow",
			],
			"settings": {
				"foreground": "#00a0a0"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Object properties",
			"scope": [
				"variable.object.property",
				"variable.other.object.property",
				"variable.other.property",
				"support.variable.property",
				"meta.object-literal.key",
				"constant.other.symbol.hashkey"
			],
			"settings": {
				"foreground": "#8000ff"
			}
		},
		{
			"name": "Meta member",
			"scope": [
				"meta.object.member",
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Variable use",
			"scope": [
				"variable.other.object",
				"variable.other.readwrite",
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Instance variables",
			"scope": [
				"variable.other.object.instance",
				"variable.other.readwrite.instance punctuation.definition.variable",
				"variable.other.readwrite.instance",
			],
			"settings": {
				"foreground": "#a8a800"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"variable.other.constant",
			],
			"settings": {
				"foreground": "#48a0ff"
			}
		},
		{
			"name": "Constant definition",
			"scope": [
				"meta.definition.variable variable.other.constant",
			],
			"settings": {
				"foreground": "#b30000"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ff9122"
			}
		},
		{
			"name": "Function calls",
			"scope": [
				"meta.function-call entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#80b000"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#0080c0"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter",
				"variable.other.block"
			],
			"settings": {
				"foreground": "#c37522"
			}
		},
		{
			"name": "Braces",
			"scope": [
				"meta.brace.round"
			],
			"settings": {
				"foreground": "#ff80ff"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#b30000"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#0080c0"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Punctuation template",
			"scope": "punctuation.definition.template-expression",
			"settings": {
				"foreground": "#bb77ff"
			}
		},
		{
			"name": "Braces {}",
			"scope": [
				"punctuation.definition.block",
				"punctuation.section.scope",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#bb77ff"
			}
		},
		{
			"name": "Brackets []",
			"scope": [
				"meta.brace.square",
				"punctuation.section.array",
			],
			"settings": {
				"foreground": "#db6262"
			}
		},
		{
			"name": "Import Components",
			"scope": "meta.import variable.other.readwrite.alias",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Import Function Components",
			"scope": "meta.import meta.block variable.other.readwrite.alias",
			"settings": {
				"foreground": "#ff9122"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"punctuation.definition.tag",
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": [
				"entity.name.tag",
				"support.class.component"
			],
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#8000ff"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "CSS: Colon",
			"scope": "punctuation.separator.key-value",
			"settings": {
				"foreground": "#b30000"
			}
		},
		{
			"name": "CSS: Color",
			"scope": "support.constant.color",
			"settings": {
				"foreground": "#80b000"
			}
		},
		{
			"name": "CSS: Function",
			"scope": "support.function",
			"settings": {
				"foreground": "#ff9122"
			}
		},
		{
			"name": "CSS: Hash",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#a8a800"
			}
		},
		{
			"name": "CSS: Ident",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "CSS: Unicode Range",
			"scope": "constant.other.unicode-range",
			"settings": {
				"foreground": "#80b000"
			}
		},
		{
			"name": "Console Yellow Output",
			"scope": "console.yellow",
			"settings": {
				"foreground": "#cdcd00"
			}
		},
		{
			"name": "Info Attributes",
			"scope": "info",
			"settings": {
				"foreground": "#d000ff",
				"fontStyle": "underline"
			}
		}
	]
}
